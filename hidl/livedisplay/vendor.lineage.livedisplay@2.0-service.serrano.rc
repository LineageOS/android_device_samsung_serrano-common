on init
    # LiveDisplay sysfs
    chown system system /sys/devices/virtual/graphics/fb0/rgb
    chmod 0660 /sys/devices/virtual/graphics/fb0/rgb

service vendor.livedisplay-hal-2-0-serrano /vendor/bin/hw/vendor.lineage.livedisplay@2.0-service.serrano
    class late_start
    user system
    group system
